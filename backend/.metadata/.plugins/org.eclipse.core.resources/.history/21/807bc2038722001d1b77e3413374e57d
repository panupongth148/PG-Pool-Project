package cdgs.co.th.resource;

import javax.persistence.Cacheable;
import javax.persistence.Entity;
import javax.persistence.NamedQuery;
import javax.persistence.QueryHint;
import javax.persistence.Table;

@Entity
@Table(name = "resource")
@NamedQuery(name = "Resource.findAll", query = "SELECT r FROM Resource r ORDER BY r.resource_id", hints = @QueryHint(name = "org.hibernate.cacheable", value = "true"))
@Cacheable
public class Resource {

}
