package cdgs.co.th.resource;

import javax.persistence.Cacheable;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.NamedQuery;
import javax.persistence.QueryHint;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

enum status {
	Y,
	N
}

@Entity
@Table(name = "resource")
@NamedQuery(name = "Resource.findAll", query = "SELECT r FROM Resource r ORDER BY r.resource_id", hints = @QueryHint(name = "org.hibernate.cacheable", value = "true"))
@Cacheable
public class Resource {
	
	 @Id
	 @SequenceGenerator(name = "resourceSequence", sequenceName = "resource_id_seq", allocationSize = 1, initialValue = 15)
	 @GeneratedValue(generator = "resourceSequence")
	 @Column(name="resource_id", updatable = false, nullable = false)
	 private Integer id;
	 
	 @Column(name="resource_name", length = 40)
	 private String resourceName;
	 
	 @Column(name="active_status")
	 private status activeStatus;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getResourceName() {
		return resourceName;
	}

	public void setResourceName(String resourceName) {
		this.resourceName = resourceName;
	}

	public status getActiveStatus() {
		return activeStatus;
	}

	public void setActiveStatus(status activeStatus) {
		this.activeStatus = activeStatus;
	}
	 
	 
}
